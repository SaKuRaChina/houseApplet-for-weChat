/**index.wxss**/


.header{position: relative;}
.header image{width: 100%;height: 280rpx;}
.searchBox{height: 50rpx;width: 450rpx;position: absolute;top: 20rpx;left: 140rpx;background: rgba(253, 127, 4, .8);border-radius: 15rpx;}
.searchBox image{width: 25rpx;height: 25rpx;margin: 12rpx 15rpx;float: left;}
.searchBox input{height: 50rpx;line-height: 50rpx;color: #fff;font-size: 22rpx;width: 380rpx;float: left}

.headerBottom{position: absolute;bottom: 10rpx;left: 0;background: rgba(0, 117, 62, .8);width: 750rpx;height: 95rpx;line-height: 95rpx;padding: 0 35rpx;}
.headerBottom navigator{display: inline-block;font-size: 30rpx;color: #fff;font-weight: bold;padding: 0 55rpx;}


.indexLog{padding: 60rpx 0 30rpx;height: 230rpx;}
.indexLog navigator{width:250rpx; float: left;text-align: center;font-size: 32rpx;}
.indexLog navigator image{width:130rpx; height: 130rpx;}
.indexLog navigator view{height: 77rpx;line-height: 77rpx;}


.fastEach{width: 750rpx;position: relative;height:480rpx;}
.fastEach image{width: 750rpx;}
.fastCont{position: absolute;top: 68rpx;left: 112rpx;width: 520rpx;height: 300rpx;background: rgba(255, 255, 255, .8);text-align: center;padding-top:10rpx; }
.fastCont text{display: block;}
.fastTitle{height:108rpx;line-height: 108rpx;font-size: 46rpx;color: #016737; }
.fastInfo{font-size: 28rpx;line-height: 41rpx;color: #878787;}
.tlPos,.trPos,.blPos,.brPos{position: absolute;width: 30rpx;height: 30rpx;}
.tlPos{border-top: 8rpx solid rgba(101, 192, 4, .8);border-left: 8rpx solid rgba(101, 192, 4, .8);top: 12rpx;left: 12rpx;}
.trPos{border-top: 8rpx solid rgba(101, 192, 4, .8);border-right: 8rpx solid rgba(101, 192, 4, .8);top: 12rpx;right: 12rpx;}
.blPos{border-bottom: 8rpx solid rgba(101, 192, 4, .8);border-left: 8rpx solid rgba(101, 192, 4, .8);bottom: 12rpx;left: 12rpx;}
.brPos{border-bottom: 8rpx solid rgba(101, 192, 4, .8);border-right: 8rpx solid rgba(101, 192, 4, .8);bottom: 12rpx;right: 12rpx;}